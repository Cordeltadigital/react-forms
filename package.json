{
  "name": "react-functional-forms",
  "version": "0.0.1",
  "description": "Build ultra simple, stateless, validated forms for use in React function components",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --runInBand",
    "build": "babel src --out-dir dist",
    "prepublish": "babel src --source-maps false --out-dir dist"
  },
  "peerDependencies": {
    "react": "^16.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.1",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.1.0",
    "babel-polyfill": "^6.26.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "jest": "^24.1.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "jest": {
    "moduleNameMapper": { "\\.css$": "<rootDir>/test/setup/styleMock.js" },
    "setupFilesAfterEnv": ["<rootDir>test/setup/beforeAll.js"]
  },
  "babel": {
    "presets": ["@babel/preset-env", "@babel/preset-react"],
    "plugins": ["@babel/plugin-proposal-class-properties"]
  },
  "keywords": ["react", "stateless", "form", "function", "component"],
  "author": "danderson00",
  "license": "MIT"
}
